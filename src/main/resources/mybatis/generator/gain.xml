<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<!-- 是否去除自动生成的注释true是false否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/hotel" userId="root"
			password="1234">
		</jdbcConnection>

		<!-- 默认false，把jdbc decimal和numeric 类型解释为integer ，为true是把 jdbc decimal和numeric 
			类型解释为java.math.BigDecimal -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- targetProject：生成po类的位置 -->
		<javaModelGenerator targetPackage="cn.com.jy.hotel.domain.gain"
			targetProject=".\src\main\java">
			<!-- 是否让schema作为包的后缀 -->
			<property name="enableSubPackages" value="true" />
			<!-- 是否从数据库返回的值被清理前后的空格 -->
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- mapper xml文件 -->
		<sqlMapGenerator targetPackage="cn.com.jy.hotel.dao.gain"
			targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- mapper接口 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="cn.com.jy.hotel.dao.gain" targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- 指定数据库表 -->
		<table tableName="h_income_day" domainObjectName="Income_Day"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<property name="useActualColumnNames" value="true" /><!-- 
			<generatedKey column="dayTime" sqlStatement="MySql"
				identity="false" /> -->
		</table>
		<table tableName="h_income_month" domainObjectName="Income_Month"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<property name="useActualColumnNames" value="true" /><!-- 
			<generatedKey column="dayTime" sqlStatement="MySql"
				identity="false" /> -->
		</table>

		<!-- 有一些表的字段需要指定java类型 -->
		<!-- <table schema="DB2ADMIN" tableName="ALLTYPES" domainObjectName="Customer" 
			> <property name="useActualColumnNames" value="true"/> <generatedKey column="ID" 
			sqlStatement="DB2" identity="true" /> <columnOverride column="DATE_FIELD" 
			property="startDate" /> <ignoreColumn column="FRED" /> <columnOverride column="LONG_VARCHAR_FIELD" 
			jdbcType="VARCHAR" /> </table> -->
	</context>
</generatorConfiguration>
