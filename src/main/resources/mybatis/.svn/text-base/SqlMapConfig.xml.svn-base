		<setting name="cacheEnabled" value="true"/>
	</settings> -->
	<!-- 只配置mapper -->
	<!--使用自动扫描器时，mapper.xml文件如果和mapper.java接口在一个目录
	则此处不用定义mappers -->
	<mappers>
		<!--同名同目录不需要此句 <mapper resource="com/mybatis/dao/dao1/User_mybatisDao1.xml"/> -->
		<!--  <package name="com.mybatis.dao"/> -->
	</mappers>
</configuration>